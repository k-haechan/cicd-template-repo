# infra_combined/terraform.tfvars.example
# 아래는 예시 값입니다. 실제 환경에 맞게 수정하세요.

project           = "sns"
domain_name       = "example.com"
region            = "ap-northeast-2"
ec2_ami           = "ami-xxxxxxxx"
ec2_instance_type = "t3.micro"
rds_instance_class = "db.t3.micro"
db_name           = "snsdb"
db_username       = "snsuser"
db_password       = "your_db_password"   # 실제 배포 시 강력한 비밀번호 사용
public_key        = "your_cloudfront_public_key"
github_owner      = "your-github-username"
repository        = "sns-backend"
email             = "admin@example.com"
github_token      = "your_github_token"  # 민감정보, 실제 배포 시 주의
aws_account_id    = "123456789012" 